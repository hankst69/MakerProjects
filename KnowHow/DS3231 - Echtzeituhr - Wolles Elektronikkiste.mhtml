    rtc.disable32K();</p>
<p>    // Making it so, that the alarm will trigger an interrupt<br>
    pinMode(CLOCK_INTERRUPT_PIN, INPUT_PULLUP);<br>
    attachInterrupt(digitalPinToInterrupt(CLOCK_INTERRUPT_PIN), onAlarm, FA=
LLING);</p>
<p>    esp_sleep_enable_ext0_wakeup(GPIO_NUM_39,0); //1 =3D High, 0 =3D Low=
</p>
